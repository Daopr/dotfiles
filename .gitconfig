[alias]
	aa = add --all
	cs = commit --signoff
	ca = cs --amend
	dc = diff --cached
	s = status
	ss = status --short
	sv = status --verbose
[branch]
	autosetuprebase = always
[core]
	autocrlf = input
	safecrlf = true
[credential]
	helper = osxkeychain
[github]
	user = nightire
[http]
	proxy = 127.0.0.1:8123
[hub]
	protocol = https
[pull]
	rebase = true
[push]
	default = simple
[status]
	branch = true
[user]
	name = Albert Yu
	email = dev.yufan@gmail.com
